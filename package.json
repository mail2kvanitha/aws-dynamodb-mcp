{
  "name": "healthcarer-mcp",
  "version": "1.0.0",
  "description": "HealthCarer MCP Server with DynamoDB",
  "main": "dist/src/server.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/server.js",
    "api": "node dist/src/api-server.js",
    "client": "node dist/client/client.js",
    "dev": "ts-node src/server.ts",
    "dev-api": "nodemon --exec ts-node src/api-server.ts",
    "test-client": "ts-node client/client.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.478.0",
    "@aws-sdk/lib-dynamodb": "^3.478.0",
    "@modelcontextprotocol/sdk": "latest",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "morgan": "^1.10.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-rate-limit": "^5.1.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.10.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  }
}
